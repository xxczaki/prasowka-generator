  "stylelint-processor-styled-components": "^1.8.0",
  "xo": "^0.25.3"
 },
 "xo": {
  "nodeVersion": ">=10",
  "parser": "babel-eslint",
  "envs": [
   "node",
   "browser"
  ],
  "extends": "xo-react",
  "settings": {
   "react": {
    "version": "16.11"
   }
  },
  "rules": {

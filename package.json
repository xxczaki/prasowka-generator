{
 "name": "prasowka-generator",
 "description": "Zbliża się lekcja WOSu, a prasówki brak? Nie ma problemu - Prasówka Generator wyszuka dla ciebie interesujące wiadomości i pomoże w utworzeniu tej bezsensownej wypowiedzi!",
 "version": "0.1.0",
 "private": true,
 "scripts": {
  "dev": "next dev",
  "build": "next build",
  "start": "next start"
 },
 "dependencies": {
  "cheerio": "^1.0.0-rc.3",
  "core-js": "3.3.4",
  "idb-keyval": "^3.2.0",
  "modern-normalize": "^0.5.0",
  "next": "9.1.1",
  "next-fonts": "^0.19.0",
  "react": "16.11.0",
  "react-dom": "16.11.0",
  "styled-components": "^5.0.0-beta.11"
 },
 "devDependencies": {
  "@babel/core": "^7.6.4",
  "@zeit/next-css": "^1.0.1",
  "babel-eslint": "^10.0.3",
  "babel-plugin-styled-components": "^1.10.6",
  "css-loader": "^3.2.0",
  "eslint-config-xo-react": "^0.20.0",
  "eslint-plugin-react": "^7.16.0",
  "eslint-plugin-react-hooks": "^2.2.0",
  "next-offline": "^4.0.6",
  "stylelint": "^11.1.1",
  "stylelint-config-recommended": "^3.0.0",
